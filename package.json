{
  "name": "larp.logistics",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "browserify": "browserify -t [ babelify --presets [ react ] ] features/**/*.js | uglifyjs > public/javascripts/bundle.js",
    "watchify": "watchify -t [babelify --presets [ react ] ] features/**/*.js -o 'uglifyjs > public/javascripts/bundle.js' ",
    "test": "mocha -R nyan tests/**/*-test.js --compilers js:babel-core/register --recursive",
    "watch": "npm run watchify & supervisor ./bin/www"
  },
  "dependencies": {
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "express-session": "^1.13.0",
    "morgan": "~1.6.1",
    "passport": "~0.3.2",
    "passport-facebook": "~2.1.0",
    "passport-google-oauth": "~1.0.0",
    "passport-local": "~1.0.0",
    "passport-twitter": "~1.0.4",
    "pg": "~4.5.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "testdom": "^2.0.0",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  }
}
